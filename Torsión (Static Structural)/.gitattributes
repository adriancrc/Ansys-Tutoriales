# -------------------------
# Git LFS (archivos grandes)
# -------------------------
*.wbpz filter=lfs diff=lfs merge=lfs -ANSYS
*.wbpj filter=lfs diff=lfs merge=lfs -ANSYS
*.rst  filter=lfs diff=lfs merge=lfs -ANSYS
*.rth  filter=lfs diff=lfs merge=lfs -ANSYS
*.cdb  filter=lfs diff=lfs merge=lfs -ANSYS
*.msh  filter=lfs diff=lfs merge=lfs -ANSYS
*.cas  filter=lfs diff=lfs merge=lfs -ANSYS
*.dat  filter=lfs diff=lfs merge=lfs -ANSYS
*.step filter=lfs diff=lfs merge=lfs -ANSYS
*.stp  filter=lfs diff=lfs merge=lfs -ANSYS
*.iges filter=lfs diff=lfs merge=lfs -ANSYS
*.igs  filter=lfs diff=lfs merge=lfs -ANSYS
*.zip  filter=lfs diff=lfs merge=lfs -ANSYS

